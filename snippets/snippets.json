{
  "smart_faq": {
    "scope": "html,php",
    "prefix": "stfaq",
    "body": [
      "<div id=\"st-faq\"></div>",
      "<script type=\"text/javascript\">",
      "  window.addEventListener('STFAQLoaded', function(e) {",
      "    e.detail.init({",
      "      \"analyticsUA\": \"${1:analyticsUA}\",",
      "      \"kbId\": ${2:kbId},",
      "      \"locale\": \"${3:locale}\",",
      "      \"cookieOptin\": ${4:cookieOptin},",
      "      \"searchFiltered\": ${5:searchFiltered},",
      "      \"headerId\": ${6:headerId},",
      "      \"buildName\": ${7:buildName},",
      "      \"filters\": {",
      "        \"thematics\": [\"${8:thematic1}\"],",
      "        \"tags\": [\"${9:tag1}\"]",
      "      }",
      "    });",
      "  });",
      "</script>",
      "<script type=\"text/javascript\"async src=\"https://assets.app.smart-tribune.com/${10:client}/FAQ/faq.main.js\"></script>"
    ],
    "description": "Insert a Smart FAQ"
  },
  "smart_push": {
    "scope": "html,php",
    "prefix": "stpush",
    "body": [
      "<script type=\"text/javascript\">",
      "  window.addEventListener('STPUSHLoaded', function(e) {",
      "    detail = e.detail;",
      "    detail.init({",
      "      \"analyticsUA\": \"${1:analyticsUA}\",",
      "      \"kbId\": ${2:kbId},",
      "      \"locale\": \"${3:locale}\",",
      "      \"cookieOptin\": ${4:cookieOptin},",
      "      \"searchFiltered\": ${5:searchFiltered},",
      "      \"entrypoint\": {",
      "        \"type\": [\"${6:type}\"],",
      "        \"value\": [\"${7:value}\"]",
      "      },",
      "      \"filters\": {",
      "        \"thematics\": [\"${8:thematic1}\"],",
      "        \"tags\": [\"${9:tag1}\"]",
      "      }",
      "    });",
      "  });",
      "</script>",
      "<script type=\"text/javascript\"async src=\"https://assets.app.smart-tribune.com/${10:client}/PUSH/push.main.js\"></script>"
    ],
    "description": "Insert a Smart Push"
  },
